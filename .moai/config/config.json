{
  "project": {
    "name": "ai-video-marketing",
    "mode": "personal",
    "locale": "ko",
    "language": "generic",
    "language_detection": {
      "detected_language": "generic",
      "detection_method": "cli_default",
      "confidence": null,
      "markers": [],
      "confirmed_by": null
    },
    "description": "{{PROJECT_DESCRIPTION}}",
    "owner": "@user",
    "created_at": "{{CREATED_AT}}",
    "initialized": true
  },
  "language": {
    "conversation_language": "ko",
    "conversation_language_name": "Korean",
    "agent_prompt_language": "en",
    "git_commit_messages": "en",
    "code_comments": "en",
    "documentation": "ko",
    "error_messages": "en",
    "notes": "Language settings: conversation_language (user interaction), agent_prompt_language (internal prompts), git_commit_messages (git commits), code_comments (source code), documentation (docs), error_messages (user error messages)"
  },
  "constitution": {
    "enforce_tdd": true,
    "principles": {
      "simplicity": {
        "max_projects": 5,
        "notes": "Default recommendation. Adjust in .moai/config.json or via SPEC/ADR with documented rationale based on project size."
      }
    },
    "test_coverage_target": 90
  },
  "git_strategy": {
    "personal": {
      "auto_checkpoint": "disabled",
      "checkpoint_events": [
        "delete",
        "refactor",
        "merge",
        "script",
        "critical-file"
      ],
      "checkpoint_type": "local-branch",
      "max_checkpoints": 10,
      "cleanup_days": 7,
      "push_to_remote": false,
      "auto_commit": true,
      "branch_prefix": "feature/SPEC-",
      "develop_branch": "develop",
      "main_branch": "main",
      "prevent_branch_creation": false,
      "work_on_main": false
    },
    "team": {
      "auto_pr": false,
      "develop_branch": "develop",
      "draft_pr": false,
      "feature_prefix": "feature/SPEC-",
      "main_branch": "main",
      "use_gitflow": true,
      "default_pr_base": "develop",
      "prevent_main_direct_merge": false
    }
  },
  "session": {
    "suppress_setup_messages": false,
    "notes": "suppress_setup_messages: false enables SessionStart output. Set to true to suppress messages (show again after 7 days)"
  },
  "moai": {
    "version": "0.30.2",
    "update_check_frequency": "daily",
    "version_check": {
      "enabled": true,
      "cache_ttl_hours": 24
    }
  },
  "user": {}
}